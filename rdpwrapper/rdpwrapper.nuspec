    <tags>admin rdp remote desktop</tags>
    <packageSourceUrl>https://github.com/echizenryoma/Chocolatey-Package/tree/master/rdpwrapper</packageSourceUrl>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>
