    <projectSourceUrl>https://github.com/x64dbg/x64dbg</projectSourceUrl>
    <mailingListUrl>https://gitter.im/x64dbg/x64dbg</mailingListUrl>
    <bugTrackerUrl>https://github.com/x64dbg/x64dbg/issues</bugTrackerUrl>
  </metadata>
  <files>
    <file src="tools\**" target="tools" />
  </files>
</package>